<!DOCTYPE html>
<html>
    <head>
      
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1"/> 
         <meta name="apple-mobile-web-app-capable" content="yes" />
        <title>{% block title %}Godart DiPrice{% endblock %}</title>
        
        {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('jquery.mobile-1.2.0.min.css') }}" />        
        {% endblock %}
        
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        

        {% block javascripts %}
       
        
        <script src="{{ asset('jquery-1.8.3.min.js') }}"></script>
        <script> 
            
            
              $(document).bind("mobileinit", function(){
             
              $.mobile.ajaxEnabled = true;     }); 
                    
          
        $( "#jqpage" ).live( 'pageshow',function(event){   
        $( "#notificationpopup" ).popup("open");});     
          
          
        
        </script>
        <script src="{{ asset('jquery.mobile-1.2.0.min.js') }}"></script>
        {% endblock %}
     

     
    
    
    </head>
    <body>
        <div data-role="page" id="jqpage">

	<div data-role="header" id="jqheader" data-theme="b"><h1>{% block jqheader%}{% endblock %}</h1></div>
  
        
	<div data-role="content" id="jqcontent" >
       {% block jqcontent %}
        {% endblock %}
        </div>
        </div>
    </body>
</html>